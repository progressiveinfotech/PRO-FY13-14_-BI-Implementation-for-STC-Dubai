    <sql>SELECT
  A.ABAL_COMP_CODE
, A.ABAL_ACNT_YEAR
, A.ABAL_CAL_YEAR

, CASE A.ABAL_CAL_MONTH
  WHEN 0 THEN 7
  ELSE A.ABAL_CAL_MONTH
