, B.ABAL_LC_UNP_CR_3
, B.ABAL_FC_UNP_DR
, B.ABAL_FC_UNP_CR
, B.ABAL_CR_UID
, B.ABAL_CR_DT
, B.ABAL_UPD_UID
, B.ABAL_UPD_DT
, B.ABAL_OP_BAL_YN
,sysdate
,to_date(1||&apos;-&apos;||(Case B.ABAL_CAL_MONTH
WHEN 0 THEN  7 
ELSE 
B.ABAL_CAL_MONTH
end )||&apos;-&apos;||B.ABAL_cal_YEAR,&apos;DD-MM-YYYY&apos;) prog_flex_dt_1

