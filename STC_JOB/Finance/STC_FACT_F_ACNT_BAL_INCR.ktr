, A.ABAL_CR_UID
, A.ABAL_CR_DT
, A.ABAL_UPD_UID
, A.ABAL_UPD_DT
, A.ABAL_OP_BAL_YN
,sysdate
,to_date(1||&apos;-&apos;||(Case A.ABAL_CAL_MONTH
WHEN 0 THEN  7 
ELSE 
A.ABAL_CAL_MONTH
end )||&apos;-&apos;||A.ABAL_cal_YEAR,&apos;DD-MM-YYYY&apos;) prog_flex_dt_1

FROM FS_CUR_ACNT_BAL A

WHERE
(A.ABAL_CR_DT &gt;= TO_DATE(&apos;${MAXETLDATE_ACNT_BAL}&apos;,&apos;DD-MM-YYYY&apos;) AND
TO_DATE(A.ABAL_CR_DT, &apos;DD-MON-YYYY&apos;) &lt; TO_DATE(SYSDATE, &apos;DD-MON-YYYY&apos;))
--to_date(A.ABAL_CR_DT,&apos;DD-MM-YYYY&apos;)&gt;=TO_DATE(&apos;1-JULY-03&apos;,&apos;DD-MM-YYYY&apos;)  and
--to_date(A.ABAL_CR_DT,&apos;DD-MM-YYYY&apos;)&lt;=TO_DATE(&apos;10-JANUARY-13&apos;,&apos;DD-MM-YYYY&apos;) 


union

SELECT
  B.ABAL_COMP_CODE
, B.ABAL_ACNT_YEAR
, B.ABAL_CAL_YEAR

, CASE B.ABAL_CAL_MONTH
  WHEN 0 THEN 7
