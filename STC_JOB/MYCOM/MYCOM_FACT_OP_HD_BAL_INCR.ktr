  <step>
    <name>Table input</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>MYCOM</connection>
    <sql>SELECT
 LTRIM(RTRIM(A.LOC_ID)) AS LOC_ID
,A.ENTRY_NO
,A.CLOSE_DATE
,A.ENTRY_DATE
,A.USER_ID
,A.POST_FLAG
,A.REMARKS
,LTRIM(RTRIM(B.LOC_ID)) AS I_LOC_ID
,B.ENTRY_NO  AS I_ENTRY_NO
,LTRIM(RTRIM(B.ITEM_CODE))  AS I_ITEM_CODE
,B.QTY_AVAIL  AS I_QTY_AVAIL
,B.AVG_COST  AS I_AVG_COST
,B.LAST_NET_COST  AS I_LAST_NET_COST
,B.CLOSE_DATE AS I_CLOSE_DATE
,B.DOC_TYPE  AS I_DOC_TYPE
,B.ADJUST_QTY  AS I_ADJUST_QTY
,LTRIM(RTRIM(D.GROUP_CODE)) AS D_GROUP_CODE
,LTRIM(RTRIM(D.SUB_GROUP)) AS D_SUB_GROUP
,D.BARCODE AS D_BARCODE
,LTRIM(RTRIM(D.SUPP_CODE)) AS D_SUPP_CODE

,LTRIM(RTRIM(D.SUB_SUBGROUP)) AS D_SUB_SUBGROUP
,LTRIM(RTRIM(D.ITEM_REF)) AS D_ITEM_REF
,D.UNIT_PACK_ID AS D_UNIT_PACK_ID

,CASE D.ITEM_TYPE
WHEN &apos;1&apos; THEN &apos;STOCK&apos;
WHEN &apos;2&apos; THEN &apos;NON_STOCK&apos;
WHEN &apos;3&apos; THEN &apos;CONSIGNMENT&apos;
END ITEM_TYPE 

,CASE D.STOCK_TYPE
WHEN &apos;1&apos; THEN &apos;REGULAR&apos;
WHEN &apos;2&apos; THEN &apos;EXPIRY&apos;
WHEN &apos;3&apos; THEN &apos;SERIALIZED&apos;
WHEN &apos;4&apos; THEN &apos;PROMOTION&apos;
END STOCK_TYPE 

,CASE D.ITEM_SCALE
WHEN &apos;0&apos; THEN &apos;NON_SCALE&apos;
WHEN &apos;1&apos; THEN &apos;SCALE&apos;
END ITEM_SCALE 

,CASE D.ITEM_STATUS
WHEN &apos;0&apos; THEN &apos;NON_SCALE&apos;
WHEN &apos;1&apos; THEN &apos;ACTIVE&apos;
WHEN &apos;2&apos; THEN &apos;DISCONTINIUE&apos;
WHEN &apos;3&apos; THEN &apos;DELETED&apos;
END ITEM_STATUS
,GETDATE()
,A.GIANT_CR_DATE 
,A.GIANT_UPD_DATE
,B.GIANT_CR_DATE AS I_GIANT_CR_DATE
,B.GIANT_UPD_DATE AS I_GIANT_UPD_DATE

FROM [STC_V4].[DBO].[OP_HD] A ,
[STC_V4].[DBO].[OP_BAL] B,
[STC_V4].[DBO].[PROD_MAST] D

WHERE 
 A.ENTRY_NO = B.ENTRY_NO AND
B.ITEM_CODE = D.ITEM_CODE and
--(A.entry_date&gt;=&apos;2013&#47;02&#47;01&apos; and A.entry_date&lt; CONVERT(VARCHAR(10),GETDATE(),111))
(A.GIANT_CR_DATE &gt;= &apos;${MAXETLDATE_MYCOM_OP_HD_BAL}&apos; AND A.GIANT_CR_DATE &lt;  CONVERT(VARCHAR(10),GETDATE(),111)
OR
A.GIANT_UPD_DATE &gt;= &apos;${MAXETLDATE_MYCOM_OP_HD_BAL}&apos; AND A.GIANT_UPD_DATE &lt; CONVERT(VARCHAR(10),GETDATE(),111)
OR
B.GIANT_UPD_DATE &gt;= &apos;${MAXETLDATE_MYCOM_OP_HD_BAL}&apos; AND B.GIANT_UPD_DATE &lt; CONVERT(VARCHAR(10),GETDATE(),111))
&#47;*
(A.GIANT_CR_DATE &gt;= &apos;2013&#47;03&#47;01&apos; AND A.GIANT_CR_DATE &lt;  GETDATE()
OR
A.GIANT_UPD_DATE &gt;= &apos;2013&#47;03&#47;01&apos; AND A.GIANT_UPD_DATE &lt;  GETDATE()
OR
B.GIANT_UPD_DATE &gt;= &apos;2013&#47;03&#47;01&apos; AND B.GIANT_UPD_DATE &lt;  GETDATE())*&#47;
</sql>
