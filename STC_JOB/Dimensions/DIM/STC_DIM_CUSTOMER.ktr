,B.ADDR_CITY_CODE AS CITY_SHORT_NAME
,B.ADDR_PROVINCE_CODE
,B.ADDR_PROVINCE_CODE AS PROVINCE_NAME
,B.ADDR_PROVINCE_CODE AS PROVIENCE_SHORT_NAME
,B.ADDR_STATE_CODE
,B.ADDR_STATE_CODE AS STATE_NAME
,B.ADDR_STATE_CODE AS STATE_SHORT_NAME
,B.ADDR_COUNTRY_CODE AS COUNTRY_NAME
,B.ADDR_COUNTRY_CODE AS COUNTRY_SHORT_NAME
,D.CA_SHIP_TO_ADDR_CODE
,D.CA_BILL_TO_ADDR_CODE,
sysdate
---,E.STATE_CODE

FROM OM_CUSTOMER A,
OM_ADDRESS B,
OM_CUST_COMP C,
OM_CUSTOMER_ADDRESS D
----OM_STATE E


WHERE 
(A.CUST_CODE=B.ADDR_CODE OR A.CUST_CODE=SUBSTR(B.ADDR_CODE,2) )
AND
A.CUST_CODE=D.CA_CUST_CODE
AND 
A.CUST_CODE = C.CCO_CUST_CODE
----B.ADDR_STATE_CODE=E.STATE_CODE
</sql>
