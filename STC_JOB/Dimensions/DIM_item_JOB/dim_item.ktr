    <sql>select 

ITEM_CODE,
ITEM_NAME,
ITEM_SHORT_NAME,
ITEM_LONG_NAME_1,
ITEM_LONG_NAME_2,
ITEM_ABC,
ITEM_VED,
ITEM_BATCH_YN_NUM,
ITEM_FRZ_FLAG_NUM,
ITEM_ANLY_CODE_01 BRAND_CODE,
ITEM_ANLY_CODE_05 COLOR_CODE,
ITEM_ANLY_CODE_16 COUNTRY_CODE,
ITEM_ANLY_CODE_10 ARTICLE_NUM,
A.AD_ANLY_TYPE BRAND_ANLY_TYPE,
A.AD_ANLY_NO BRAND_AD_ANLY_NO,
A.AD_CODE BRAND_AD_CODE,
A.AD_NAME BRAND_AD_NAME,
A.AD_SHORT_NAME BRAND_AD_SHORT_NAME,
B.AD_ANLY_TYPE COLOR_AD_ANLY_TYPE,
B.AD_ANLY_NO COLOR_AD_ANLY_NO,
B.AD_CODE COLOR_AD_CODE,
B.AD_NAME COLOR_AD_NAME,
B.AD_SHORT_NAME COLOR_AD_SHORT_NAME,

NVL(D.AD_ANLY_TYPE,&apos;NA&apos;) COUNTRY_AD_ANLY_TYPE,
NVL(D.AD_ANLY_NO,0) COUNTRY_AD_ANLY_NO,
NVL(D.AD_CODE,&apos;NA&apos;) COUNTRY_AD_CODE,
NVL(D.AD_NAME,&apos;NA&apos;) COUNTRY_AD_NAME,
NVL(D.AD_SHORT_NAME,&apos;NA&apos;) COUNTRY_AD_SHORT_NAME,
SYSDATE

from  OM_ITEM C
left join  OM_ANALYSIS_DETAIL A on (C.ITEM_ANLY_CODE_01=A.AD_CODE And A.AD_ANLY_TYPE = &apos;ITEM&apos;
and A.AD_ANLY_NO= 1 )
left join  OM_ANALYSIS_DETAIL B on (c.ITEM_ANLY_CODE_05=B.AD_CODE and B.AD_ANLY_TYPE = &apos;ITEM&apos;
and B.AD_ANLY_NO= 5)
left join  OM_ANALYSIS_DETAIL D on (c.ITEM_ANLY_CODE_16=D.AD_CODE and D.AD_ANLY_TYPE = &apos;ITEM&apos;
and D.AD_ANLY_NO= 16)
left join  OM_ANALYSIS_DETAIL E on (c.ITEM_ANLY_CODE_10=E.AD_CODE and E.AD_ANLY_TYPE = &apos;ITEM&apos;
and E.AD_ANLY_NO= 10)
--WHERE C.ITEM_CR_DT &gt;= TO_DATE(&apos;11-FEB-2009&apos;, &apos;DD-MON-YY&apos;)
--OR C.ITEM_UPD_DT &gt;= TO_DATE(&apos;11-FEB-2009&apos;, &apos;DD-MON-YY&apos;)
where C.ITEM_CR_DT &gt;= TO_DATE(&apos;${MAXETLDATE}&apos;, &apos;DD-MM-YYYY&apos;)
or C.ITEM_UPD_DT &gt;= TO_DATE(&apos;${MAXETLDATE}&apos;, &apos;DD-MM-YYYY&apos;)

--where 
--C.ITEM_CODE=&apos;011040214&apos;</sql>
