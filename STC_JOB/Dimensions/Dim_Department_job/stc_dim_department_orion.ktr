    <sql>select distinct DEPT_CODE, DEP_NAME,section_code,section_name,d.AD_CODE Sub_Section_code, d.AD_NAME sub_section_name, 
sysdate, &apos;O&apos; AS FLEX_08 
from 
    (select distinct 
            a.AD_CODE  DEPT_CODE, a.AD_NAME DEP_NAME,
            b.AD_CODE section_code, b.AD_NAME section_name      
    from         
            (select AD_PARENT_CODE , AD_CODE, AD_NAME from OM_ANALYSIS_HEAD,OM_ANALYSIS_DETAIL
            WHERE  AD_ANLY_NO = ANLY_NO 
            AND ANLY_NO=6 and ANLY_TYPE=&apos;ITEM&apos;) b,
            (select AD_PARENT_CODE , AD_CODE, AD_NAME from OM_ANALYSIS_HEAD,OM_ANALYSIS_DETAIL
            WHERE  AD_ANLY_NO = ANLY_NO 
            AND ANLY_NO=2 and ANLY_TYPE=&apos;ITEM&apos;)  a
    where  B.AD_PARENT_CODE =A.AD_CODE 
    ) c,
    (select AD_PARENT_CODE , AD_CODE, AD_NAME,AD_REMARK from OM_ANALYSIS_HEAD,OM_ANALYSIS_DETAIL
    WHERE  AD_ANLY_NO = ANLY_NO 
    AND ANLY_NO=7 and ANLY_TYPE=&apos;ITEM&apos;) d
where c.section_code = d.AD_PARENT_CODE
  AND D.AD_REMARK =DEPT_CODE
  --AND DEPT_CODE IN(&apos;75&apos;)
order by 1,2,3,4,5,6
</sql>
