  PARTY_UPD_DT               ,
  PARTY_UPD_UID              ,
  PARTY_AGING_ON_ACCOUNT     ,
  SYSDATE

 FROM OS_PARTY_AGING_SUMMARY 

WHERE&#47;*
(PARTY_CR_DATE &gt;= TO_DATE(&apos;${MAXETLDATE_CUST_SALE_RET_BATCH}&apos;,&apos;DD-MM-YYYY&apos;) AND
TO_DATE(PARTY_CR_DATE, &apos;DD-MON-YYYY&apos;) &lt; TO_DATE(SYSDATE, &apos;DD-MON-YYYY&apos;)
OR 
PARTY_UPD_DATE &gt;= TO_DATE(&apos;${MAXETLDATE_CUST_SALE_RET_BATCH}&apos;,&apos;DD-MM-YYYY&apos;) AND 
TO_DATE(PARTY_UPD_DATE, &apos;DD-MON-YYYY&apos;) &lt; TO_DATE(SYSDATE, &apos;DD-MON-YYYY&apos;))
*&#47;
PARTY_CR_DATE &gt;= TO_DATE(&apos;1&#47;JUL&#47;2009&apos;, &apos;DD&#47;MM&#47;YYYY&apos;) 
AND PARTY_CR_DATE &lt; TO_DATE(&apos;11&#47;JAN&#47;2013&apos;, &apos;DD&#47;MM&#47;YYYY&apos;) </sql>
